{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-test.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^~/(.*)$": "<rootDir>/../src/$1"
  },
  "setupFiles": ["<rootDir>/../src/jest-setup.ts"]
}
